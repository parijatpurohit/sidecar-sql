// protobuf for defining schema of {{ .TableName }}
syntax = "proto3";
{{ range .Imports }}
import "{{ . }}";
{{ end }}
package storage;
option go_package = "pb";

message {{ .TableName }} {
    {{ range .Fields }}
    {{- .FieldType }} {{ .FieldName }} = {{ .FieldIndex }};
    {{ end }}
}

message {{ .PKConfig.Name }} {
{{- range .PKConfig.Fields }}
    {{ if .IsRepeated}}repeated {{end}} {{ .FieldType }} {{ .FieldName }} = {{ .FieldIndex }};
{{- end }}
}